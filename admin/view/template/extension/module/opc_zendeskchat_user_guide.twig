{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <a href="{{ cancel }}" data-toggle="tooltip" title="Back To Module" class="btn btn-default"><i class="fa fa-reply"></i></a>
      </div>
      <h1>Opencart Zendesk Chat Integration User Guide</h1>
    </div>
  </div>
  <div class="container-fluid">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Opencart Zendesk Chat Integration User Guide</h3>
      </div>
      <div class="panel-body">
        {% for i in 1..10 %}
          <div class="form-group">
            <h1>Step {{ i }}: </h1><img class="img-thumbnail" src="image/opc_zendeskchat_step{{ i }}.png" alt="" />
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>
{{ footer }}
