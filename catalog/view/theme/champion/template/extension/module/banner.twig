<section class="container-marketing my-5">
  <div class="container marketing">
    <div class="our-partner">
      <h3 class="featurette-heading font-weight-bolder text-primary text-center my-5">OUR PARTNERS</h3>
      <div class="custom-manufacturer-slider">
        <div class="owl-slider">
          <div id="carousel" class="owl-carousel">
              {% for banner in banners %}
            <div class="item">
                {% if banner.link %}
                  <a href="{{ banner.link }}">
                    <img data-src="{{ banner.image }}" alt="{{ banner.title }}" class="owl-lazy" />
                  </a>
                {% else %}
                  <img data-src="{{ banner.image }}" alt="{{ banner.title }}" class="owl-lazy" />
                {% endif %}

            </div>
              {% endfor %}

          </div>
        </div>
      </div>

    </div>
  </div>
</section>

<script>
    $(document).ready(function () {
    $("#carousel").owlCarousel({
        autoplay: true,
        lazyLoad: true,
        loop: true,
        margin: 20,
        /*
       animateOut: 'fadeOut',
       animateIn: 'fadeIn',
       */
        responsiveClass: true,
        autoHeight: true,
        autoplayTimeout: 7000,
        smartSpeed: 800,
        nav: true,
        responsive: {
            0: {
                items: 1
            },

            600: {
                items: 3
            },

            1024: {
                items: 10
            },

            1366: {
                items: 10
            }
        }
    });
    });

</script>
